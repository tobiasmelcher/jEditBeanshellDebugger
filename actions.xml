<?xml version="1.0"?>
<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>
    <ACTION NAME="bshdebugger.openview-action">
        <CODE>
            bshdebugger.DebuggerView.set(view);
        </CODE>
    </ACTION>
    
    <ACTION NAME="bshdebugger.togglebreakpoint-action">
        <CODE>
            bshdebugger.DebuggerView.toggleBreakpoint(view);
        </CODE>
    </ACTION>
    
    <ACTION NAME="bshdebugger.stepinto-action">
        <CODE>
            bshdebugger.DebuggerView.doStep();
        </CODE>
    </ACTION>
    
    <ACTION NAME="bshdebugger.continue-action">
        <CODE>
            bshdebugger.DebuggerView.doContinue();
        </CODE>
    </ACTION>
    
</ACTIONS>
